<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Library">

    <!-- White Color -->
    <Color x:Key="CustomWhite">#F1F1F1</Color>
    <SolidColorBrush x:Key="CustomWhiteBrush" Color="{StaticResource CustomWhite}" />
    
    <!-- Main Blue Color -->
    <Color x:Key="MainBlue">#6A789F</Color>
    <SolidColorBrush x:Key="MainBlueBrush" Color="{StaticResource MainBlue}" />
    
    <!-- Dark Blue Color -->
    <Color x:Key="DarkBlue">#404860</Color>
    <SolidColorBrush x:Key="DarkBlueBrush" Color="{StaticResource DarkBlue}" />
    
    <!-- Light Blue Color -->
    <Color x:Key="LightBlue">#A7AEC4</Color>
    <SolidColorBrush x:Key="LightBlueBrush" Color="{StaticResource LightBlue}" />

    <!-- Grey Color -->
    <Color x:Key="CustomGrey">#707070</Color>
    <SolidColorBrush x:Key="CustomGreyBrush" Color="{StaticResource CustomGrey}" />
    
    <!-- Medium Grey Color -->
    <Color x:Key="MediumGrey">#8E8E8E</Color>
    <SolidColorBrush x:Key="MediumGreyBrush" Color="{StaticResource MediumGrey}" />
    
    <!-- Medium Light Grey Color -->
    <Color x:Key="MediumLightGrey">#D8D5D5</Color>
    <SolidColorBrush x:Key="MediumLightGreyBrush" Color="{StaticResource MediumLightGrey}" />
    
    <!-- Light Grey Color -->
    <Color x:Key="CustomLightGrey">#EDEDED</Color>
    <SolidColorBrush x:Key="CustomLightGreyBrush" Color="{StaticResource CustomLightGrey}" />
    
    <!-- Green Color -->
    <Color x:Key="CustomGreen">#4C884D</Color>
    <SolidColorBrush x:Key="CustomGreenBrush" Color="{StaticResource CustomGreen}" />
    
    <!-- Hover Green -->
    <Color x:Key="HoverGreen">#455E45</Color>
    <SolidColorBrush x:Key="HoverGreenBrush" Color="{StaticResource HoverGreen}" />

    <!-- Wine Red Color -->
    <Color x:Key="WineRed">#651919</Color>
    <SolidColorBrush x:Key="WineRedBrush" Color="{StaticResource WineRed}" />

    <!-- Hover Red -->
    <Color x:Key="HoverRed">#2B0303</Color>
    <SolidColorBrush x:Key="HoverRedBrush" Color="{StaticResource HoverRed}" />

</ResourceDictionary>