<Application x:Class="Library.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Library">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styling\Styles\Colors.xaml" />
                <ResourceDictionary Source="Styling\Styles\Fonts.xaml" />
                <ResourceDictionary Source="Styling\Styles\Buttons.xaml" />
                <ResourceDictionary Source="Styling\Styles\Texts.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
